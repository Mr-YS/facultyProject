				$thead = $('<thead>');
				$tbody = $('<tbody>');
				for(var i = 0; i < 10; ++i) {
					$tr = $('<tr>');
 					for(var j = 0; j < 10; ++j) {
  						$td = $('<td>');
  						$tr.append($td);
 					}
 					$tbody.append($tr);
				}
				$table.append($thead);
				$table.append($tbody);
